  "version": "0.0.0",
  "scripts": {
    "dev": "vite --mode development",
    "prod": "vite --mode production",
    "startd": "rimraf dist && vite build --mode development && node app.js --development",
    "startp": "rimraf dist && vite build --mode production && node app.js --production",
    "clear": "rimraf dist ",
    "build-t": "rimraf dist && vite build --mode production  && node zip.js --production",
    "build-prod": "rimraf dist && vite build --mode production",
    "build-dev": "rimraf dist && vite build --mode development",
    "build-all": "rimraf dist && vite build --mode production && vite build --mode development",
    "bb-build": "rimraf dist && babel src -d dist/client  --ignore=\"node_modules\"",
    "preview": "vite preview"
  },
